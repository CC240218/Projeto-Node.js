<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Notícias</title>
		</head>
	<body>

		<h1>Notícias</h1>
<table>
<tr>
	<td><h3>Identificador</h3></td>
	<td><h3>Título</h3></td>
	<td><h3>Noticia</h3></td>
</tr>
<% for(var i = 0; i< noticias.length; i++){ %>
<tr>
	<td> <%= noticias[i].id_noticia %> </td>
	<td> <%= noticias[i].titulo %> </td>
	<td> <%= noticias[i].noticia %> </td>
</tr>
<% } %>
</table>
	</body>
</html>